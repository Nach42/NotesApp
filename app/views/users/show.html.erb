<div class="text-center">
	<h1><%=@user.name%>'s profile</h1>
</div>
<div class="row large-padding">
	<p>
	  <strong>Name:</strong>
	  <%= @user.name %>
	</p>

	<p>
	  <strong>Email:</strong>
	  <%= @user.email %>
	</p>

	<%= link_to 'Back', welcome_path %>
	<% if session[:user]== @user.id %>
	<%= link_to 'Edit', edit_user_path(@user) %>
	<%end%>
</div>