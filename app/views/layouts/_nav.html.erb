<header>
  <nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <div class="navbar-header">
        <%= link_to "NotesApp", welcome_path, class: "navbar-brand" %>
      </div>
        <ul class="nav navbar-nav">
          <% if session[:user] %>
            <li class="active"><%= link_to "My Notes", user_notes_path(session[:user]) %></li>
            <li><%= link_to "My Collections", user_collections_path(session[:user]) %></li>
            <li><%= link_to "Friends", welcome_path %></li>
          </ul>
              <div class="user_nav">
      </div>
              <ul class="nav navbar-nav navbar-right">
                 <% if session[:user] %>
                    <li><%= link_to session[:user_name], user_path(session[:user]) %></li>
                    <li><%= link_to "Logout", logout_path %></li>
                  <% else %>
                    <li><%= link_to "Sign up", signup_path %> or
                    <li><%= link_to "log in", login_path %>.
                  <% end %>   
              </ul>
            </div>
          <% end %>
        </ul>
    </div>
  </nav>
</header>